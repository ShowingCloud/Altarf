<h1>Listing addresses</h1>

<table>
  <tr>
    <th>Membership</th>
    <th>Username</th>
    <th>Mobile</th>
    <th>Phone</th>
    <th>Province</th>
    <th>City</th>
    <th>District</th>
    <th>Postal</th>
    <th>Addr</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @addresses.each do |address| %>
  <tr>
    <td><%= address.membership_id %></td>
    <td><%= address.username %></td>
    <td><%= address.mobile %></td>
    <td><%= address.phone %></td>
    <td><%= address.province %></td>
    <td><%= address.city %></td>
    <td><%= address.district %></td>
    <td><%= address.postal %></td>
    <td><%= address.addr %></td>
    <td><%= link_to 'Show', address %></td>
    <td><%= link_to 'Edit', edit_address_path(address) %></td>
    <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Address', new_address_path %>
